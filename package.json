{
  "name": "responsive-grid",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Demola Adebayo",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "start:dev": "nodemon --exec babel-node server/ --watch",
    "dev:start": "cross-env NODE_ENV=development babel-node server --colors  --useServerRender=true --watch",
    "concur": "concurrently \"yarn run dev:start\"  \"yarn run webhook\" ",
    "dev:build": "webpack --config webpack.config.dev.babel.js --progress --profile --colors -wd",
    "serve": "live-server public/",
    "dev-server": "webpack-dev-server",
    "webhook": "lt -p 8080 -s influxidea",
    "test": "jest --watchAll"
  },
  "pre-commit": [
    "test"
  ],
  "babel": {
    "presets": [
      "react",
      "env",
      "stage-2"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "babel-eslint": "^8.2.3",
